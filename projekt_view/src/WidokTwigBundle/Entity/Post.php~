<?php

namespace WidokTwigBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

    /**
     * @ORM\Entity
     * @ORM\Table(name = "posts")
     */

class Post {
    
    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue 
     */
    private $id;
    
    /**
     *  @ORM\Column(type="string", length = 100)
     */
    
    private $title;
    private $text;
    

}
